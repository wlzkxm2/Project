<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="733dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/btn_MyPage"
                android:layout_width="58dp"
                android:layout_height="match_parent"
                android:layout_weight="0"
                android:background="@color/white"
                app:srcCompat="@drawable/mainuser" />
                android:outlineAmbientShadowColor="@color/white"
                android:scaleType="fitCenter"
                app:srcCompat="@drawable/user" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="51dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/img" />

            <ImageButton
                android:id="@+id/imageButton6"
                android:layout_width="58dp"
                android:layout_height="match_parent"
                android:layout_weight="0"
                android:background="@color/white"
                android:outlineAmbientShadowColor="@color/white"
                app:srcCompat="@drawable/notification" />

        </LinearLayout>

        <EditText
            android:id="@+id/editTextTextPersonName2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="검색어 입력"
            android:inputType="textPersonName"
            android:text="" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="565dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <ImageButton
                    android:id="@+id/mib01"
                    android:layout_width="150dp"
                    android:layout_height="200dp"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="17dp"
                    android:layout_marginBottom="10dp"
                    android:layout_weight="0"
                    android:background="@color/white"
                    android:outlineAmbientShadowColor="@color/white"
                    app:layout_constraintBottom_toTopOf="@+id/mib03"
                    app:layout_constraintEnd_toStartOf="@+id/mib02"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/ipad" />

                <ImageButton
                    android:id="@+id/mib02"
                    android:layout_width="212dp"
                    android:layout_height="200dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginBottom="10dp"
                    android:background="@color/white"
                    app:layout_constraintBottom_toTopOf="@+id/mib03"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/mib01"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/lenovo" />

                <ImageButton
                    android:id="@+id/mib03"
                    android:layout_width="407dp"
                    android:layout_height="136dp"
                    android:layout_marginStart="2dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="2dp"
                    android:background="@color/white"
                    app:layout_constraintBottom_toTopOf="@+id/imageButton7"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/mib01"
                    app:srcCompat="@drawable/iphone" />

                <ImageButton
                    android:id="@+id/imageButton7"
                    android:layout_width="362dp"
                    android:layout_height="187dp"
                    android:layout_marginStart="34dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="15dp"
                    android:layout_marginBottom="1dp"
                    android:background="@color/white"
                    app:layout_constraintBottom_toTopOf="@+id/imageButton9"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/mib03"
                    app:srcCompat="@drawable/squidgame" />

                <ImageButton
                    android:id="@+id/imageButton8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="49dp"
                    android:layout_marginTop="3dp"
                    android:layout_marginEnd="24dp"
                    android:layout_marginBottom="16dp"
                    android:background="@color/white"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/imageButton9"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageButton7"
                    app:srcCompat="@drawable/cola" />

                <ImageButton
                    android:id="@+id/imageButton9"
                    android:layout_width="138dp"
                    android:layout_height="269dp"
                    android:layout_marginStart="24dp"
                    android:layout_marginEnd="51dp"
                    android:background="@color/white"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/imageButton8"
                    app:layout_constraintTop_toBottomOf="@+id/imageButton7"
                    app:srcCompat="@drawable/pepsi" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </ScrollView>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/imageButton3"
                android:layout_width="58dp"
                android:layout_height="match_parent"
                android:layout_weight="0"
                android:background="@color/white"
                android:outlineAmbientShadowColor="@color/white"
                app:srcCompat="@drawable/event" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="294dp"
                android:layout_height="match_parent"
                android:layout_weight="0"
                android:text="" />

            <ImageButton
                android:id="@+id/imageButton4"
                android:layout_width="58dp"
                android:layout_height="match_parent"
                android:layout_weight="0"
                android:background="@color/white"
                android:outlineAmbientShadowColor="@color/white"
                app:srcCompat="@drawable/menu" />
        </LinearLayout>

    </LinearLayout>

</LinearLayout>