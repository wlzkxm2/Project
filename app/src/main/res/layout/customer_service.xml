<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tex"
        android:layout_width="120dp"
        android:layout_height="31dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="200dp"
        android:scaleX="2"
        android:scaleY="2"
        android:text="고객센터"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
        app:layout_constraintVertical_bias="0.019" />

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="408dp"
        android:layout_height="119dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-24dp">

        <ImageButton
            android:id="@drawable/back"
            android:layout_width="58dp"
            android:layout_height="53dp"
            android:layout_marginTop="28dp"
            android:background="@color/white"
            android:outlineAmbientShadowColor="@color/white"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/back" />

        <ImageView
            android:id="@drawable/img"
            android:layout_width="287dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            app:srcCompat="@drawable/img" />
    </LinearLayout>

    <ImageView
        android:id="@drawable/rounded_rectangle"
        android:layout_width="397dp"
        android:layout_height="266dp"
        android:scaleType="fitXY"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="106dp"
        tools:srcCompat="@drawable/rounded_rectangle" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="100dp"
        android:layout_height="137dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/account"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="167dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="181dp"
        android:layout_height="104dp"
        android:text="LCHE 이용시 불편한 점 궁금한 점을 말씀해 주세요!"
        android:textColor="@color/black"
        android:textsize="30dp"
        tools:layout_editor_absoluteX="169dp"
        tools:layout_editor_absoluteY="182dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="403dp"
        android:layout_height="39dp"
        android:text="1:1문의"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="372dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="397dp"
        android:layout_height="42dp"
        android:text="Q&A"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="411dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="389dp"
        android:layout_height="43dp"
        android:text="공지사항"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="537dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="395dp"
        android:layout_height="41dp"
        android:text="이용약관"
        tools:layout_editor_absoluteX="9dp"
        tools:layout_editor_absoluteY="461dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="391dp"
        android:layout_height="39dp"
        android:text="LCHE 소개"
        tools:layout_editor_absoluteX="9dp"
        tools:layout_editor_absoluteY="498dp" />

</androidx.constraintlayout.widget.ConstraintLayout>